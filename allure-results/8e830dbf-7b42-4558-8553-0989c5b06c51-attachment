  14 |     readonly lastNameValues = (lastname: string) => this.page.locator(`div.oxd-table-cell:has-text("${lastname}")`).first()
  15 |     readonly editEmployeeInfoIconBtn: Locator
  16 |     readonly successPopUp: Locator
